{
  "name": "delegatte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui-add": "turbo run ui-add",
    "backend:setup": "turbo -F @delegatte/backend setup",
    "backend:dev": "turbo -F @delegatte/backend dev"
  },
  "devDependencies": {
    "@delegatte/eslint-config": "workspace:*",
    "@delegatte/tsconfig": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
